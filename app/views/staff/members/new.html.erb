<% @page_title = "Create Member" %>

<%= link_to("&laquo; Back to List".html_safe, staff_members_path, :class => 'back-link') %>

<div class="member new">
  <h1><%= @page_title %></h1>

  <%= form_for([:staff, @member]) do |f| %>
    <%= render(:partial => "form", :locals => {:f => f}) %>
    <div id="operations">
      <%= submit_tag("Create Member") %>
      <%= cancel_button(staff_members_path) %>
    </div>
  <% end %>
</div>
